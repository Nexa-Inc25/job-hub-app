{
  "name": "job-hub-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.5",
    "@mui/material": "^5.14.18",
    "@mui/x-tree-view": "^7.0.0",
    "@vercel/speed-insights": "^1.3.1",
    "axios": "^1.6.2",
    "pdf-lib": "^1.17.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-pdf": "^9.2.1",
    "react-router-dom": "^6.21.1",
    "react-scripts": "^5.0.1",
    "recharts": "^3.7.0",
    "socket.io-client": "^4.8.3",
    "web-vitals": "^5.1.0",
    "yaml": "^2.8.2"
  },
  "devDependencies": {
    "@craco/craco": "^7.1.0",
    "@testing-library/cypress": "^10.1.0",
    "@testing-library/jest-dom": "^6.4.0",
    "@testing-library/react": "^14.2.0",
    "@testing-library/user-event": "^14.5.0",
    "autoprefixer": "^10.4.16",
    "cypress": "^15.9.0",
    "serve": "^14.2.4",
    "start-server-and-test": "^2.1.3",
    "tailwindcss": "^3.4.1"
  },
  "scripts": {
    "start": "craco start",
    "build": "GENERATE_SOURCEMAP=false craco build",
    "test": "craco test",
    "test:coverage": "craco test --coverage --watchAll=false",
    "test:ci": "CI=true craco test --coverage --watchAll=false",
    "eject": "react-scripts eject",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:run:headed": "cypress run --headed",
    "serve:build": "serve -s build -l 3000",
    "e2e": "start-server-and-test start http://localhost:3000 cypress:run",
    "e2e:ci": "npm run build && start-server-and-test serve:build http://localhost:3000 cypress:run",
    "e2e:open": "start-server-and-test start http://localhost:3000 cypress:open"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "globals": {
      "globalThis": "readonly"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:5000",
  "overrides": {
    "tar": "^7.0.0",
    "nth-check": "^2.1.1",
    "postcss": "^8.4.31",
    "webpack-dev-server": "^5.2.1"
  }
}
